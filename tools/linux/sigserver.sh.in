#!/bin/sh -x

if [ -z $LD_LIBRARY_PATH ]
then
    LD_LIBRARY_PATH=@JVM_PATH@
else
    LD_LIBRARY_PATH=@JVM_PATH@:$LD_LIBRARY_PATH
fi
SIGVERSE_X3DPARSER_CONFIG=@INSTALL_DIR@/share/etc/X3DParser.cfg
SIGVERSE_RUNAC=@INSTALL_DIR@/bin/sigrunac
SIGVERSE_DATADIR=@INSTALL_DIR@/share/data
export LD_LIBRARY_PATH SIGVERSE_X3DPARSER_CONFIG SIGVERSE_RUNAC SIGVERSE_DATADIR

@INSTALL_DIR@/bin/sigserver $@
